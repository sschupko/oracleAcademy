<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>shop.controllers.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RegistrationServlet</servlet-name>
    <servlet-class>shop.controllers.RegistrationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/Login</url-pattern>
    <url-pattern>/pages/Login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegistrationServlet</servlet-name>
    <url-pattern>/Registration</url-pattern>
    <url-pattern>/pages/Registration</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>/pages/index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>LogoutServlet</display-name>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>shop.controllers.LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/Logout</url-pattern>
    <url-pattern>/pages/Logout</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>UserManager</display-name>
    <servlet-name>UserManager</servlet-name>
    <servlet-class>shop.controllers.UserManager</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserManager</servlet-name>
    <url-pattern>/pages/UserManager</url-pattern>
     <url-pattern>/UserManager</url-pattern>
  </servlet-mapping>
</web-app>